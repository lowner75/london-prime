{
  "name": "londonprime",
  "version": "1.0.0",
  "description": "London Prime",
  "main": "app.js",
  "private": true,
  "author": "Bryan Lown",
  "license": "ISC",
  "scripts": {
    "start": "npm run watch:all",
    "app": "node app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "sass public/css/:public/css/ --watch",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run app\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts -f node_modules/@openfonts/open-sans_all/files/*300* dist/fonts -f node_modules/@openfonts/open-sans_all/files/*400* -f node_modules/@openfonts/open-sans_all/files/* dist/fonts -f node_modules/@openfonts/nunito-sans_all/files/* dist/fonts -f node_modules/@openfonts/roboto_all/files/* dist/fonts -f node_modules/@openfonts/roboto-condensed_all/files/* dist/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin views/index.ejs -d dist -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lowner75/london-prime.git"
  },
  "bugs": {
    "url": "https://github.com/lowner75/london-prime/issues"
  },
  "homepage": "https://github.com/lowner75/london-prime#readme",
  "devDependencies": {
    "@openfonts/cormorant-garamond_all": "^1.44.2",
    "@openfonts/nunito-sans_all": "^1.44.2",
    "@openfonts/open-sans_all": "^1.44.2",
    "@openfonts/roboto-condensed_all": "^1.44.2",
    "@openfonts/roboto_all": "^1.44.1",
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "http-server": "^0.12.3",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "lite-server": "^2.6.1",
    "nodemon": "^2.0.6",
    "onchange": "^3.3.0",
    "open": "^7.3.0",
    "open-sans-fonts": "^1.6.2",
    "parallelshell": "^3.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7",
    "trumpet": "^1.7.2",
    "uglify-js": "^3.12.4",
    "uglifyjs": "^2.4.11",
    "usemin": "^0.6.0",
    "usemin-cli": "^0.6.0",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "fontsource-nunito-sans": "^4.0.0",
    "fontsource-open-sans": "^4.0.0",
    "gsap": "^3.6.0",
    "hamburgers": "^1.1.3",
    "jquery": "^3.3.1",
    "lightgallery.js": "^1.4.0",
    "luxy.js": "^0.1.0",
    "nodemailer": "^6.4.18",
    "popper.js": "^1.12.9"
  }
}
